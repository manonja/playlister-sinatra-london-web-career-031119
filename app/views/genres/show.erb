<h4><%= @genre.name%></h4>

<h4>Artist(s):</h4>

<% @genre.artists.each do |artist|  %>
  <li> <a href="/artists/<%= artist.slug%>">
    <%= artist.name %>
  </a> </li>
<% end %>

<h4>Song(s):</h4>
<% @genre.songs.each do |song|  %>
  <li> <a href="/songs/<%= song.slug%>">
    <%= song.name %>
  </a> </li>
<% end %>
